    <script async src="https://www.googletagmanager.com/gtag/js?id=UA-39410406-3"></script>
    <script>
        window.dataLayer = window.dataLayer || [];
        function gtag(){dataLayer.push(arguments);}
        gtag('js', new Date());

        gtag('config', 'UA-39410406-3');
    </script>
    <script async src="{{ baseUrl }}/js/autotrack.js"></script>
